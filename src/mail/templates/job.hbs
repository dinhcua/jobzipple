<html>
  <head>
    <meta charset='utf-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
  </head>
  <body>
    <div class='container'>
      <div>{{name}}</div>
      {{#each jobs}}
        <div>name: {{name}}</div>
        <div>company: {{company}}</div>
        <div>salary: {{salary}}</div>
        <div>skills: {{#each skills}}{{this}} ,{{/each}}</div>
      {{/each}}
    </div>
  </body>
</html>